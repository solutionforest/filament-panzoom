{
    "name": "solution-forest/filament-panzoom",
    "description": "This is my package filament-panzoom",
    "keywords": [
        "SolutionForest",
        "laravel",
        "filament-panzoom"
    ],
    "homepage": "https://github.com/solutionforest/filament-panzoom",
    "support": {
        "issues": "https://github.com/solutionforest/filament-panzoom/issues",
        "source": "https://github.com/solutionforest/filament-panzoom"
    },
    "license": "MIT",
    "authors": [{
        "name": "Jacky",
        "email": "info+package@solutionforest.net",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.3",
        "filament/filament": "^3.3|^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SolutionForest\\FilamentPanzoom\\": "src/",
            "SolutionForest\\FilamentPanzoom\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolutionForest\\FilamentPanzoom\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SolutionForest\\FilamentPanzoom\\FilamentPanzoomServiceProvider"
            ],
            "aliases": {
                "FilamentPanzoom": "SolutionForest\\FilamentPanzoom\\Facades\\FilamentPanzoom"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}